{
  "name": "Cloudflare DDNS updater",
  "version": "1.1.1.2",
  "slug": "ddns",
  "url": "https://github.com/kjell5317/addon-ddns",
  "description": "Updates the public IP address to all A records on listed zones on Cloudflare",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "services",
  "boot": "auto",
  "options": {
    "zones": [
      {
        "api_key": null,
        "zone_id": null
      }
    ],
    "update": 300
  },
  "schema": {
    "zones": [
      {
        "api_key": "str",
        "zone_id": "str"
      }
    ],
    "update": "int"
  },
  "map": ["config"]
}
